<?xml version="1.0" encoding="UTF-8" ?>
        <!ELEMENT organizations (organization)+>
        <!ELEMENT organization (name, adress, telephone, email, ads)>
        <!ATTLIST organization
                login ID #REQUIRED
                password CDATA #REQUIRED
                >
        <!ELEMENT name (#PCDATA)>
        <!ELEMENT adress (#PCDATA)>
        <!ELEMENT telephone (#PCDATA)>
        <!ELEMENT email (#PCDATA)>
        <!ELEMENT ads (ad)*>
        <!ELEMENT ad (title, discription, sampler_type, sampler_title, productivity, price)>
        <!ELEMENT title (#PCDATA)>
        <!ELEMENT discription (#PCDATA)>
        <!ELEMENT sampler_type (#PCDATA)>
        <!ELEMENT sampler_title (#PCDATA)>
        <!ELEMENT productivity (#PCDATA)>
        <!ELEMENT price (#PCDATA)>



